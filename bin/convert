#! /usr/bin/env node

const convertToLowResolution = require("../modules/media/utils/convertToLowResolution");

try {
  const filename = process.argv[2];
  if (!filename.endsWith("mp4")) {
    throw new Error("Media must be video");
  }
  convertToLowResolution(filename);
} catch (error) {
  console.log(error.message);
}
